{
    "name": "lojatemdetudo",
    "version": "1.0.0",
    "description": "Loja tem de tudo versao web",
    "main": "app.js",
    "scripts": {
        "build": "NODE_ENV=production webpack",
        "lint:front": "eslint client/src",
        "lint:back": "eslint server",
        "dev:front": "webpack --watch",
        "dev:back": "nodemon app.js",
        "dev": "concurrently --raw --kill-others \"npm run dev:front\" \"npm run dev:back\"",
        "start": "node app.js",
        "test": "tape client/src/**/**/*.spec.js server/**/**/*.spec.js | tap-spec",
        "test:watch": "tape-watch client/src/**/**/*.spec.js server/**/**/*.spec.js | tap-spec",
        "lint:fix": "eslint client/src/**/*.{js,vue} server --fix"
    },
    "private": true,
    "author": "IBM CIOM LA",
    "license": "ISC",
    "dependencies": {
        "body-parser": "^1.18.2",
        "eslint-plugin-vue": "^4.7.1",
        "express": "^4.16.3",
        "log4js": "^3.0.5",
        "vue": "^2.5.16",
        "vue-router": "^3.0.1"
    },
    "devDependencies": {
        "autoprefixer-loader": "^3.2.0",
        "babel-core": "^6.26.0",
        "babel-eslint": "^8.2.3",
        "babel-loader": "^7.1.4",
        "babel-preset-airbnb": "^2.4.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "concurrently": "^3.5.1",
        "css-loader": "^0.28.11",
        "eslint": "^4.19.1",
        "eslint-config-airbnb": "^16.1.0",
        "eslint-plugin-import": "^2.11.0",
        "eslint-plugin-jsx-a11y": "^6.0.3",
        "eslint-plugin-react": "^7.7.0",
        "extract-text-webpack-plugin": "^3.0.2",
        "file-loader": "^1.1.11",
        "html-webpack-plugin": "^3.2.0",
        "node-sass": "^4.8.3",
        "nodemon": "^1.17.3",
        "sass-loader": "^7.0.1",
        "sinon": "^4.5.0",
        "style-loader": "^0.21.0",
        "supertest": "^3.0.0",
        "svg-loader": "0.0.2",
        "tap-spec": "^5.0.0",
        "tape": "^4.9.0",
        "tape-watch": "^2.3.0",
        "vue-loader": "^14.2.2",
        "vue-style-loader": "^4.1.0",
        "vue-template-compiler": "^2.5.16",
        "webpack": "^4.4.1",
        "webpack-cli": "^3.3.12",
        "webpack-dev-middleware": "^3.4.0",
        "webpack-hot-middleware": "^2.22.3"
    },
    "engines": {
        "node": "8.11.3",
        "npm": "6.2.0"
    }
}
